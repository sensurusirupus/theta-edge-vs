{
  "root": "lib/webview",
  "targets": {
    "compile": {
      "dependsOn": ["^build"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "lib/webview",
        "command": "tsc"
      }
    },
    "build": {
      "dependsOn": ["compile"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "lib/webview",
        "command": "npx esbuild build/webview.js --external:vscode '--define:process.env.NODE_ENV=\"production\"' --bundle --platform=browser --outfile=dist/webview.js"
      }
    }
  }
}
